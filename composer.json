{
  "name": "robsox/stylist",
  "description": "Laravel 5,6,7,8,9 theming package.",
  "authors": [
    {
      "name": "Kirk Bushell",
      "email": "torm3nt@gmail.com"
    },
    {
      "name": "Mike Dugan",
      "email": "mike@mjdugan.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
    "laravel/framework": "^6.0|^7.0|^8.0|^9.0",
    "laravelcollective/html": "^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "FloatingPoint\\Stylist\\": "src/Stylist",
      "Tests\\": "tests"
    }
  },
  "laravel": {
      "providers": [
        "FloatingPoint\\Stylist\\StylistServiceProvider"
      ],
      "aliases": {
        "Stylist": "FloatingPoint\\Stylist\\Facades\\Stylist",
        "Theme": "FloatingPoint\\Stylist\\Facades\\Theme"
      }
    }
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist"
  }
}
